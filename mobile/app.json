{
	"expo": {
		"name": "Chie",
		"slug": "Chie",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "native",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.ordyboii.chie"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#f5f5dc"
			},
			"edgeToEdgeEnabled": true,
			"package": "com.ordyboii.chie"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/icon.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#f5f5dc"
				}
			],
			[
				"expo-font",
				{
					"fonts": [
						"node_modules/@expo-google-fonts/noto-sans/400Regular.otf",
						"node_modules/@expo-google-fonts/noto-sans/700Bold.otf"
					]
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		}
	}
}
