{
  "expo": {
    "name": "Jbot",
    "slug": "jbot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ordyboii.jbot"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ecd2a6"
      },
      "package": "com.ordyboii.jbot"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 700,
          "resizeMode": "contain",
          "backgroundColor": "#ecd2a6",
          "dark": {
            "image": "./assets/images/splash-icon-dark.png",
            "backgroundColor": "#282828"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e5ebe4eb-f9f3-4300-a8e8-7c4ddd32669a"
      }
    }
  }
}
