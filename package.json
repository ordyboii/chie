{
  "name": "jbot",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "node --import tsx ./src/app.ts",
    "dev": "node --watch --import tsx src/app.ts",
    "format": "prettier . --write",
    "lint": "eslint . --ext .ts",
    "prepare": "husky"
  },
  "lint-staged": {
    "*": [
      "npm run format",
      "npm run lint"
    ]
  },
  "dependencies": {
    "@slack/bolt": "^4.2.0",
    "bullmq": "^5.39.1",
    "ioredis": "^5.5.0",
    "replicate": "^1.0.1",
    "tsx": "^4.19.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/node": "^22.10.10",
    "eslint": "^9.21.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.2",
    "typescript-eslint": "^8.24.1"
  }
}
